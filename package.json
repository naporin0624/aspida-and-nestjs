{
  "name": "nest-and-aspida",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist && tsc -p tsconfig.check.json",
    "build:server": "nest build", 
    "build:views": "webpack --config webpack.config.ts",
    "build": "npm-run-all -p build:server build:views",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cd dist && node server/main",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build:api": "rimraf apis && openapi2aspida --build"
  },
  "dependencies": {
    "@aspida/axios": "^0.4.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.1",
    "async-retry": "^1.3.1",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "html-webpack-plugin": "^4.0.2",
    "nest-spa-serve": "latest",
    "openapi2aspida": "^0.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.9.4",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/async-retry": "^1.4.1",
    "@types/express": "^4.17.3",
    "@types/html-minifier-terser": "^5.0.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/rimraf": "^3.0.0",
    "@types/supertest": "^2.0.8",
    "@types/webpack": "^4.41.8",
    "@types/webpack-dev-server": "^3.10.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "aspida-mock": "^0.4.1",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "server",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
